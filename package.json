{
  "name": "diara",
  "version": "1.0.0",
  "description": "Banish your abandoned GitHub repositories - Extra damage to all undead repos",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "diara": "./bin/diara.ts"
  },
  "scripts": {
    "start": "bun src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun build src/index.ts --compile --outfile dist/diara",
    "build:macos-arm": "bun build src/index.ts --compile --target=bun-darwin-arm64 --outfile dist/diara-macos-arm64",
    "build:macos-x64": "bun build src/index.ts --compile --target=bun-darwin-x64 --outfile dist/diara-macos-x64",
    "build:linux-x64": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile dist/diara-linux-x64",
    "build:linux-arm": "bun build src/index.ts --compile --target=bun-linux-arm64 --outfile dist/diara-linux-arm64",
    "build:all": "bun run build:macos-arm && bun run build:macos-x64 && bun run build:linux-x64 && bun run build:linux-arm"
  },
  "keywords": [
    "github",
    "cli",
    "repository",
    "delete",
    "cleanup",
    "bulk",
    "bun",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "@octokit/rest": "^21.0.0",
    "chalk": "^5.3.0",
    "conf": "^13.0.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "bun-types": "^1.3.8",
    "typescript": "^5.9.3"
  }
}
